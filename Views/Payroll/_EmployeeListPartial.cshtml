@model  List<Employee>

   <table class="table">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
    </tr>
  </thead>
  <tbody>
    <tr>
@foreach (var employee in Model)
{
     <tr>
         <td>@employee.Id</td>
      <td>@employee.Name</td>
      <td>@employee.Email</td>
    </tr>
    
}
</tbody>
</table>